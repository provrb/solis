{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Solis",
  "version": "0.1.0",
  "identifier": "com.solis.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Solis",
        "width": 1180,
        "height": 700,
        "decorations": false,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/solis-32x32-1.png",
      "icons/solis-128x128-1.png",
      "icons/solis-128x128-2.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
